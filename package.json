{
  "name": "ilocation-spoofer",
  "version": "0.0.1",
  "description": "iOS Location Spoofer allows the GPS coordinates of iOS devices to be modified, empowering users to protect their location privacy.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run build-main-dev\" \"npm run start-renderer-dev\" \"wait-on ./build/app.js && wait-on tcp:3000 && npm run start-main-dev\"",
    "build-main-dev": "webpack --watch --config ./configs/main/webpack.config.dev.ts",
    "start-main-dev": "cd ./build && cross-env ELECTRON_ENV=dev electron app.js",
    "start-renderer-dev": "webpack-dev-server --port 3000 --config ./configs/renderer/webpack.config.dev.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arevi/iLocation-Spoofer.git"
  },
  "author": "Armin Dizdarevic",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/arevi/iLocation-Spoofer/issues"
  },
  "homepage": "https://github.com/arevi/iLocation-Spoofer#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "electron": "^15.2.0",
    "electron-builder": "^22.13.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.4.0",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.2.17",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
